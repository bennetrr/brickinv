{
  "name": "brickinv",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsc -m node16 vite.dev-server.ts && node vite.dev-server.js",
    "dev:local": "cp public/env/env.local.js public/env/env.js && yarn dev",
    "dev:prod": "cp public/env/env.prod.js public/env/env.js && yarn dev",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "docs:gen": "cosmos --expose-imports",
    "docs:dev": "yarn docs:gen & yarn dev --root=docs && fg"
  },
  "engines": {
    "node": ">=21.0.0"
  },
  "dependencies": {
    "@dicebear/collection": "^8.0.0",
    "@dicebear/core": "^8.0.0",
    "@fontsource-variable/inter": "^5.0.16",
    "@heroicons/react": "^2.1.1",
    "@wemogy/reactbase": "^0.9.8",
    "axios": "^1.6.7",
    "debug": "^4.3.4",
    "eslint-plugin-mobx": "^0.0.9",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "mobx": "^6.12.0",
    "mobx-react": "^9.1.0",
    "mobx-state-tree": "^5.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-images-uploading": "^3.1.7",
    "react-router-dom": "^6.21.3",
    "react-spinners": "^0.13.8",
    "react-use-clipboard": "^1.0.9",
    "supports-color": "^9.4.0",
    "the-new-css-reset": "^1.11.2"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/lodash": "^4.14.202",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.11",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "chokidar": "^3.6.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.8.0",
    "react-cosmos": "^6.1.1",
    "react-cosmos-dom": "^6.1.1",
    "react-cosmos-plugin-boolean-input": "^6.1.1",
    "react-cosmos-plugin-open-fixture": "^6.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "yargs": "^17.7.2"
  }
}
